<p id="notice"><%= notice %></p>

<h1>Submitted Requests</h1>

<table>
  <thead>
    <tr>
      <th>Belongs to</th>
      <th>Belongs to</th>
      <th>Belongs to</th>
      <th>Belongs to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submitted_requests.each do |submitted_request| %>
      <tr>
        <td><%= submitted_request.belongs_to %></td>
        <td><%= submitted_request.belongs_to %></td>
        <td><%= submitted_request.belongs_to %></td>
        <td><%= submitted_request.belongs_to %></td>
        <td><%= link_to 'Show', submitted_request %></td>
        <td><%= link_to 'Edit', edit_submitted_request_path(submitted_request) %></td>
        <td><%= link_to 'Destroy', submitted_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

