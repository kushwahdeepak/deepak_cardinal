<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>
    
  </div>
</nav>


  <div id="main-content">
<h1>Ownerships</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ownerships.each do |ownership| %>
      <tr>
        <td><%= link_to 'Show', ownership %></td>
        <td><%= link_to 'Edit', edit_ownership_path(ownership) %></td>
        <td><%= link_to 'Destroy', ownership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ownership', new_ownership_path %>
</div>

