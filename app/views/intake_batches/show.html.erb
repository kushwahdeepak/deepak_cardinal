<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>

  </div>
</nav>


<div id="main-content">
  <p><strong>Status:</strong><%= @intake_batch.user_message %></p>
  <p><%= link_to('download the file back', rails_blob_path(@intake_batch.original_file, disposition: "attachment")) %></p>
  <p></p><%= link_to 'Back', '/' %></p>

  <%if @show_log %>
    <p><strong>Log</strong></p>
    <ul class="aaa">
    <%@intake_batch.log.split("\n").each do |l| %>
      <li><%= l %></li>
        <hr>
    <% end if @intake_batch.log.present? %>
    </ul>
  <%end %>
</div>
