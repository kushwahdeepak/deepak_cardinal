<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Recipient</th>
      <th scope="col">Contact at</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
  	<% @campaign_recipients.each do |recipient| %>
    <tr>
      <th scope="row"><%= recipient.id %></th>
      <td><%= recipient.recipient.name %></td>
      <td><%= recipient.contact_recipient_at %></td>
      <td><%= recipient.status %></td>
    </tr>
    <% end %>
  </tbody>
</table>