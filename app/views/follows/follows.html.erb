<!-- erbstart app/views/follows/follows.html.erb -->
<div class="space-90"></div>
<div style="min-height: fit-content;">
  <p id="notice"><%= notice %></p>
  <div id="side-bar">
    
  </div>

  <div id="main-content clearfix">

    <table>
      <thead>
        <tr>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @follows.each do |follow| %>
          <tr>
            <td><%= link_to 'Show', follow %></td>
            <td><%= link_to 'Edit', edit_follow_path(follow) %></td>
            <td><%= link_to 'Destroy', follow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>

    <br>

    <%= link_to 'New Follow', new_follow_path %>
  </div>
<!-- erbend app/views/follows/follows.html.erb -->
