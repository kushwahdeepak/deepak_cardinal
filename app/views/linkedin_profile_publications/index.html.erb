<!-- erbstart app/views/linkedin_profile_publications/index.html.erb -->
<p id="notice"><%= notice %></p>

<h1>Linkedin Profile Publications</h1>

<table>
  <thead>
    <tr>
      <th>Linkedin profile</th>
      <th>Title</th>
      <th>Date</th>
      <th>Date year</th>
      <th>Date month</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @linkedin_profile_publications.each do |linkedin_profile_publication| %>
      <tr>
        <td><%= linkedin_profile_publication.linkedin_profile_id %></td>
        <td><%= linkedin_profile_publication.title %></td>
        <td><%= linkedin_profile_publication.date %></td>
        <td><%= linkedin_profile_publication.date_year %></td>
        <td><%= linkedin_profile_publication.date_month %></td>
        <td><%= link_to 'Show', linkedin_profile_publication %></td>
        <td><%= link_to 'Edit', edit_linkedin_profile_publication_path(linkedin_profile_publication) %></td>
        <td><%= link_to 'Destroy', linkedin_profile_publication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Linkedin Profile Publication', new_linkedin_profile_publication_path %>
