<p id="notice"><%= notice %></p>

<h1>Linkedin Field Of Studies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @linkedin_field_of_studies.each do |linkedin_field_of_study| %>
      <tr>
        <td><%= linkedin_field_of_study.name %></td>
        <td><%= link_to 'Show', linkedin_field_of_study %></td>
        <td><%= link_to 'Edit', edit_linkedin_field_of_study_path(linkedin_field_of_study) %></td>
        <td><%= link_to 'Destroy', linkedin_field_of_study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Linkedin Field Of Study', new_linkedin_field_of_study_path %>
