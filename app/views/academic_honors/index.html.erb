<!-- erbstart app/views/academic_honors/index.html.erb -->
<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>

  </div>
</nav>


<div id="main-content">
<h1>Academic Honors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @academic_honors.each do |academic_honor| %>
      <tr>
        <td><%= link_to 'Show', academic_honor %></td>
        <td><%= link_to 'Edit', edit_academic_honor_path(academic_honor) %></td>
        <td><%= link_to 'Destroy', academic_honor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Academic Honor', new_academic_honor_path %>
</div>
<!-- erbend app/views/academic_honors/index.html.erb -->
