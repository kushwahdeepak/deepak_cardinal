<!-- erbstart app/views/accrediting_institutions/index.html.erb -->
<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>
    
  </div>
</nav>


  <div id="main-content">

<h1>Accrediting Institutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accrediting_institutions.each do |accrediting_institution| %>
      <tr>
        <td><%= link_to 'Show', accrediting_institution %></td>
        <td><%= link_to 'Edit', edit_accrediting_institution_path(accrediting_institution) %></td>
        <td><%= link_to 'Destroy', accrediting_institution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accrediting Institution', new_accrediting_institution_path %>

</div>

