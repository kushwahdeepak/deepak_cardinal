<!-- erbstart app/views/linkedin_profile_educations/index.html.erb -->
<p id="notice"><%= notice %></p>

<h1>Linkedin Profile Educations</h1>

<table>
  <thead>
    <tr>
      <th>Linkedin profile</th>
      <th>School name</th>
      <th>Field of study</th>
      <th>Degree</th>
      <th>Start date</th>
      <th>Start date year</th>
      <th>Start date month</th>
      <th>End date</th>
      <th>End date year</th>
      <th>End date month</th>
      <th>Activities</th>
      <th>Notes</th>
      <th>Linkedin field of study</th>
      <th>Linkedin school</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @linkedin_profile_educations.each do |linkedin_profile_education| %>
      <tr>
        <td><%= linkedin_profile_education.linkedin_profile_id %></td>
        <td><%= linkedin_profile_education.school_name %></td>
        <td><%= linkedin_profile_education.field_of_study %></td>
        <td><%= linkedin_profile_education.degree %></td>
        <td><%= linkedin_profile_education.start_date %></td>
        <td><%= linkedin_profile_education.start_date_year %></td>
        <td><%= linkedin_profile_education.start_date_month %></td>
        <td><%= linkedin_profile_education.end_date %></td>
        <td><%= linkedin_profile_education.end_date_year %></td>
        <td><%= linkedin_profile_education.end_date_month %></td>
        <td><%= linkedin_profile_education.activities %></td>
        <td><%= linkedin_profile_education.notes %></td>
        <td><%= linkedin_profile_education.linkedin_field_of_study_id %></td>
        <td><%= linkedin_profile_education.linkedin_school_id %></td>
        <td><%= link_to 'Show', linkedin_profile_education %></td>
        <td><%= link_to 'Edit', edit_linkedin_profile_education_path(linkedin_profile_education) %></td>
        <td><%= link_to 'Destroy', linkedin_profile_education, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Linkedin Profile Education', new_linkedin_profile_education_path %>
