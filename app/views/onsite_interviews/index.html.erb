<p id="notice"><%= notice %></p>

<h1>Onsite Interviews</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @onsite_interviews.each do |onsite_interview| %>
      <tr>
        <td><%= link_to 'Show', onsite_interview %></td>
        <td><%= link_to 'Edit', edit_onsite_interview_path(onsite_interview) %></td>
        <td><%= link_to 'Destroy', onsite_interview, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Onsite Interview', new_onsite_interview_path %>
