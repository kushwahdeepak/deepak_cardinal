<!-- erbstart app/views/user_experiences/_form.html.erb -->
<%= simple_form_for(@user_experience, html: {class: 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_name %>
    <%= f.input :title %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :description %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
<!-- erbend app/views/user_experiences/_form.html.erb -->
