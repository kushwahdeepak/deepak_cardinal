<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>
    
  </div>
</nav>


  <div id="main-content">

<h1>Account Sources</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_sources.each do |account_source| %>
      <tr>
        <td><%= link_to 'Show', account_source %></td>
        <td><%= link_to 'Edit', edit_account_source_path(account_source) %></td>
        <td><%= link_to 'Destroy', account_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account Source', new_account_source_path %>

</div>

