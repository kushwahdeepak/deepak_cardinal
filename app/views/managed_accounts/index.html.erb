<!-- erbstart app/views/managed_accounts/index.html.erb -->
<p id="notice"><%= notice %></p>

<h1>Managed Accounts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @managed_accounts.each do |managed_account| %>
      <tr>
        <td><%= link_to 'Show', managed_account %></td>
        <td><%= link_to 'Edit', edit_managed_account_path(managed_account) %></td>
        <td><%= link_to 'Destroy', managed_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Managed Account', new_managed_account_path %>
