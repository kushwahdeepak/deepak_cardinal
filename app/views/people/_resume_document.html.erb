<% if @user.resume.exists? %>
  <%= link_to "Resume Document", @user.resume.url, target: :_blank %>
  <iframe src="https://docs.google.com/viewer?url=https:<%= @user.resume.url %>&embedded=true" width="100%" height="800" frameborder="0" >
    <p>Your browser does not support iframes.</p>
  </iframe>
<% end %>
