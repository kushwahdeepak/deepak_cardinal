<!-- erbstart app/views/workflow_items/index.html.erb -->
<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>
    
  </div>
</nav>


  <div id="main-content">
<h1>Workflow Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workflow_items.each do |workflow_item| %>
      <tr>
        <td><%= link_to 'Show', workflow_item %></td>
        <td><%= link_to 'Edit', edit_workflow_item_path(workflow_item) %></td>
        <td><%= link_to 'Destroy', workflow_item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workflow Item', new_workflow_item_path %>

</div>

