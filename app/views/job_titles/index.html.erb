<!-- erbstart app/views/job_titles/index.html.erb -->
<div class="space-180"></div>

<nav>
  <div id="side-bar">
    <div class="space-40"></div>
    
  </div>
</nav>


  <div id="main-content">
<h1>Job Titles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @job_titles.each do |job_title| %>
      <tr>
        <td><%= link_to 'Show', job_title %></td>
        <td><%= link_to 'Edit', edit_job_title_path(job_title) %></td>
        <td><%= link_to 'Destroy', job_title, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job Title', new_job_title_path %>

</div>

